!Emacs.geometry: 120x55
! Emacs.Font:     Terminus
!Emacs*Background: #0b0b0b
!Emacs*Foreground: #dcdcdc
!Emacs.bitmapIcon: off
!Emacs.verticalScrollBars: off
!Emacs.toolBar: off
!Emacs.cursorBlink: off
Emacs.fontBackend: ftcr

! Hint: use
!       fc-list :spacing=mono | sort
URxvt.font:     xft:bitstream vera sans mono:size=10:antialias=true
URxvt.boldFont: xft:bitstream vera sans mono:bold:size=10:antialias=true
URxvt*color0:  #000000 // black
URxvt*color1:  #B40000 // red
URxvt*color2:  #00AA00 // green
URxvt*color3:  #AAAA00 // yellow
URxvt*color4:  #5555AA // blue
URxvt*color5:  #AA00AA // magenta
URxvt*color6:  #00AAAA // cyan
URxvt*color7:  #AAAAAA // white
URxvt*color8:  #555555 // black (now and below: bright)
URxvt*color9:  #FF0000 // red
URxvt*color10: #00FF00 // green
URxvt*color11: #FFFF00 // yellow
URxvt*color12: #6464FF // blue
URxvt*color13: #FF00FF // magenta
URxvt*color14: #00FFFF // cyan
URxvt*color15: #FFFFFF // white

URxvt.foreground: white
URxvt.background: black
URxvt.fading: 50

URxvt.geometry: 110x40
URxvt.imLocale: de_DE
URxvt.insecure: 1
URxvt.internalBorder: 2
URxvt.mapAlert: true
URxvt.meta8: false
URxvt.print-pipe: cat /tmp/print
URxvt.saveLines: 3000
URxvt.scrollBar_right: 1
URxvt.scrollstyle: plain
URxvt.utmpInhibit: true
URxvt.visualBell: true
URxvt.jumpScroll: true
URxvt.skipScroll: true
URxvt.scrollWithBuffer: false

URxvt.cutchars: ()*,<>[]{}|
URxvt.perl-ext: selection,option-popup,selection-popup,readline,matcher
URxvt.perl-ext-common: selection,option-popup,selection-popup,readline,matcher
URxvt.url-launcher: /usr/bin/x-www-browser
! URxvt.url-launcher: /usr/bin/xdg-open
URxvt.matcher.button: 1
URxvt*matcher.pattern.1: \\b(http|https|ftp):[/]*[\\w-]\\.[\\w./?&@#-]*[\\w/-]
URxvt*matcher.pattern.2: \\bwww\\.[\\w-]\\.[\\w./?&@#-]*[\\w/-]
URxvt.matcher.rend.0: fg8



color0: #000000
color1: #cb1578
color2: #8ecb15
color3: #cb9a15
color4: #6f15cb
color5: #cb15c9
color6: #15b4cb
color7: #888a85
color8: #465457
color9: #dc5e86
color10: #9edc60
color11: #dcb65e
color12: #7e5edc
color13: #b75edc
color14: #5edcb4
color15: #ffffff
background : #0e0021
foreground : #bcbcbc


Xft.autohint:       false
Xft.lcdfilter: lcddefault
Xft.hintstyle: hintsligth
Xft.hinting:         true
Xft.antialias:       true
Xft.rgba:             rgb
Xft.dpi:               96

Xcursor.theme:    Neutral
