# Defaults
Host *
	ControlMaster auto
	ControlPath ~/.ssh/sockets/ssh_mux_%h_%p_%r
	ControlPersist 120
	ForwardX11 no
	Cipher arcfour
	Compression yes
	CompressionLevel 4

Host mde mde19? 192.168.233.*
Host bitbucket.org
	ForwardX11 no
	ForwardAgent no
	ControlMaster no

Host qemu
	User root
	Hostname localhost
	ControlMaster auto
	Port 12345
	CheckHostIP no
	HashKnownHosts no
	
Host 192.168.233.199
	User root
	HashKnownHosts no
	CheckHostIP no
			
Host mde
	User root
	HashKnownHosts no
	CheckHostIP no
	
